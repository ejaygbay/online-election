                                    <div class="align-self-center ">
                                        <img src="../public/images/user.jpg" class="img-fluid text-info fa-3x" />
                                    </div>
                                    <div class="text-end ">
                                        <h3>423</h3>
                                        <p class="mb-0 ">Votes</p>
                                    </div>
                                </div>
                                <div class="px-md-1 ">
                                    <div class="progress mt-3 mb-1 rounded " style="height: 15px ">
                                        <div class="progress-bar bg-danger " role="progressbar " style="width: 40% " aria-valuenow="40 " aria-valuemin="0 " aria-valuemax="100 ">10%</div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
